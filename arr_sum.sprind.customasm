#include "./sprind.customasm"
#bank rom

#align 4
arr_sum: ; subr(arr_addr: *i16, len: u16) -> i16
	subi sp, 2
	sw [sp][0], s0

	; alias arr_addr => x
	; alias len => y
	; alias sum => z

	b .cond

	.loop:
		decr y
		lw z, [x][y]
		add s0, z

		.cond:
			cmpi y, 0
			bg .loop

	mov x, s0

	lw s0, [sp][0]
	addi sp, 2
	ret


                  